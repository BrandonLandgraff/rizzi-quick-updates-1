        "default": "pos-side-image-left", 
        "options": [ 
          {"value": "pos-side-image-left", "label": "Left"},
          {"value": "pos-side-image-right", "label": "Right"}
        ]
      },
      {
        "type": "range",
        "id": "side_image_height",
        "min": 200,
        "max": 1000,
        "step": 100,
        "unit": "px",
        "label": "Side image height",
        "default": 500
      },
      {
        "type": "range",
        "id": "side_image_width",
        "min": 25,
        "max": 400,
        "step": 25,
        "unit": "px",
        "label": "Side image width",
        "default": 200
      },
      {
        "type": "range",
        "id": "mb_side_image_height",
        "min": 200,
        "max": 1000,
        "step": 100,
        "unit": "px",
        "label": "Mobile side image height",
        "default": 500
      },
      {
        "type": "range",
        "id": "mb_side_image_width",
        "min": 25,
        "max": 400,
        "step": 25,
        "unit": "px",
        "label": "Mobile side image width",
        "default": 200
      },
      {
        "type": "header",
        "content": "Video"
      },
      {
      "type": "text",
      "id": "video_link_bg",
      "label": "Video background link",
      "info": "MP4 video link from [files](/admin/settings/files)."
      },
      {
        "type": "select",
        "id": "desktop_height",
        "label": "Desktop height",
        "default": "screen-height-three-quarters",
        "options": [
          { "value": "screen-height-full", "label": "Full screen height"},
          { "value": "screen-height-three-quarters", "label": "3/4 of screen"},
          { "value": "screen-height-two-thirds", "label": "2/3 of screen"},
          { "value": "screen-height-one-half", "label": "1/2 of screen"},
          { "value": "screen-height-one-third", "label": "1/3 of screen"},
          { "value": "seven-fifty-height-hero", "label": "750px" },
          { "value": "sixty-fifty-height-hero", "label": "650px" },
          { "value": "five-fifty-height-hero ", "label": "550px" },
          { "value": "four-fifty-height-hero", "label": "450px" },
          { "value": "image-height", "label": "Image height" }
        ]
      },
      {
        "type": "select",
        "id": "mobile_height",
        "label": "Mobile height",
        "default": "screen-height-three-quarters--mobile",
        "options": [
          { "value": "screen-height-full--mobile", "label": "Full screen height"},
          { "value": "screen-height-three-quarters--mobile", "label": "3/4 of screen"},
          { "value": "screen-height-two-thirds--mobile", "label": "2/3 of screen"},
          { "value": "screen-height-one-half--mobile", "label": "1/2 of screen"},
          { "value": "screen-height-one-third--mobile", "label": "1/3 of screen"},
          { "value": "seven-fifty-height-hero--mobile", "label": "750px" },
          { "value": "sixty-fifty-height-hero--mobile", "label": "650px" },
          { "value": "five-fifty-height-hero--mobile", "label": "550px" },
          { "value": "four-fifty-height-hero--mobile", "label": "450px" },
          { "value": "image-height--mobile", "label": "Image height" }
        ]
      }
    ],
    "max_blocks": 8,
    "blocks": [
      {
        "type": "subtitle",
        "name": "Subtitle",
        "settings": [
                {
                "id": "subtitle",
                "type": "text",
                "label": "Subtitle"
                }
            ]
        },
        {
        "type": "header",
        "name": "Header",
        "settings": [
                {
                "id": "header",
                "type": "text",
                "label": "Header"
                }
            ]
        },
        {
          "type": "richtext",
          "name": "Description",
          "settings": [
              {
                "type": "richtext",
                "id": "richtext",
                "label": "Description",
                "default": "<p>This is a description.</p>"
              }
            ]
        },
        {
        "type": "bullet",
        "name": "Bullet",
        "settings": [
                {
                "id": "bullet",
                "type": "text",
                "label": "Bullet"
